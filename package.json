{
  "name": "vuepress-plugin-export",
  "version": "0.0.0",
  "description": "Export your VuePress site to a PDF file",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "licenses": "MIT",
  "scripts": {
    "export": "vuepress export example",
    "test": "npm run lint && jest",
    "lint": "xo"
  },
  "repository": {
    "url": "ulivz/vuepress-plugin-export",
    "type": "git"
  },
  "author": "ulivz<chl814@foxmail.com>",
  "license": "MIT",
  "dependencies": {
    "puppeteer": "^1.11.0",
    "easy-pdf-merge": "^0.2.0"
  },
  "devDependencies": {
    "xo": "0.23.0",
    "eslint-config-sherry": "0.0.1",
    "husky": "1.2.0",
    "lint-staged": "8.1.0",
    "jest": "23.6.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "xo": {
    "extends": [
      "sherry"
    ],
    "envs": [
      "jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "xo --fix",
      "git add"
    ]
  },
  "peerDependencies": {
    "vuepress": ">=1.0.0-alpha.33"
  }
}
